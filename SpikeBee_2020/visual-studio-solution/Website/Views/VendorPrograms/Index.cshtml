@model IEnumerable<Website.Models.VendorProgram>
@using Microsoft.AspNetCore.Identity
@using Website.Data

@inject SignInManager<ApplicationUser> SignInManager

@{
    ViewData["Title"] = "Index";
}

<div class="container page-alignment">
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Vendor.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ProgramName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Headline)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.RegistrationStartDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.RegistrationEndDate)
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Vendor.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ProgramName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Headline)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.RegistrationStartDate)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.RegistrationEndDate)
                    </td>
                    <td>
                        <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                        <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
                                                                             